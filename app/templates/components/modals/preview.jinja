<div class="modal fade" id="modal-preview" style="display: none;" aria-modal="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-body">
                <div class="modal-title">{{ demo.get('name')}}</div>
                <dl class="row">
                    {% for attribute in demo.get('preview') %}
                    <dt class="col-5">{{ attribute }}:</dt>
                    <dd class="col-7">{{ demo.get('preview').get(attribute) }}</dd>
                    {% endfor %}
                </dl>
            </div>
        </div>
    </div>
</div>